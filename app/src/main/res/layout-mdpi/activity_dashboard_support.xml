<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">
    <ScrollView
        android:layout_width="match_parent"
        android:scrollbars="none"
        android:layout_height="match_parent">

        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent">




    <RelativeLayout
        android:id="@+id/rlsupportToolbarId"
        android:layout_width="match_parent"
        android:layout_height="@dimen/_110sdp"
        android:background="@drawable/relativeredius">

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/ivdashboardsupportbackId"
            android:layout_width="@dimen/_30sdp"
            android:layout_height="@dimen/_28sdp"
            android:layout_marginStart="@dimen/_10sdp"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="10sp"
            android:padding="5sp"
            android:src="@drawable/back" />

        <androidx.appcompat.widget.AppCompatImageView
            android:id="@+id/ivnotificationsupportId"
            android:layout_width="@dimen/_28sdp"
            android:layout_height="@dimen/_25sdp"
            android:layout_alignParentRight="true"
            android:layout_marginTop="25dp"
            android:layout_marginEnd="15sp"
            android:src="@drawable/ring" />

        <androidx.appcompat.widget.AppCompatTextView
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/ivnotificationsupportId"
            android:layout_centerHorizontal="true"
            android:layout_marginTop="25dp"
            android:text="@string/support_and_contact"
            android:textColor="@color/tabselected"
            android:textSize="28dp"
            android:textStyle="normal" />


    </RelativeLayout>

    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_below="@+id/rlsupportToolbarId">

        <RelativeLayout
            android:id="@+id/rlsupportid"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_margin="15sp">


            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvcontactid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/contact_form"
                android:textColor="@color/Black"
                android:textSize="@dimen/_14sdp"
                android:textStyle="normal" />


            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvcontactid"
                android:layout_marginTop="@dimen/_5sdp"
                android:text="@string/please_precise_the_subject_of_your_contact_in_the_title"
                android:textColor="@color/tabselected"
                android:textSize="15sp"
                android:textStyle="normal" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rltitleid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rlsupportid"
            android:layout_marginLeft="25dp"
            android:layout_marginTop="10sp"
            android:layout_marginRight="25dp"
            android:background="@drawable/support_box">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvtitleidd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_margin="15sp"
                android:text="@string/title"
                android:textColor="@color/Black"
                android:textSize="16dp"
                android:textStyle="bold" />

            <androidx.appcompat.widget.AppCompatEditText
                android:layout_width="match_parent"
                android:id="@+id/etTitleSupportId"
                android:layout_height="wrap_content"
                android:layout_marginTop="15sp"
                android:layout_toEndOf="@+id/tvtitleidd"
                android:background="@android:color/transparent"
                android:singleLine="true"
                android:textColor="@color/lessBlack"
                android:textSize="14sp"
                android:textStyle="normal" />

        </RelativeLayout>

        <RelativeLayout
            android:id="@+id/rldescriptionid"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rltitleid"
            android:layout_marginLeft="10sp"
            android:layout_marginTop="10sp"
            android:layout_marginRight="10sp"
            android:background="@drawable/descriptionbox">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvdescriptionidd"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginEnd="15sp"
                android:layout_marginStart="15sp"
                android:layout_marginTop="15sp"
                android:text="@string/description"
                android:textColor="@color/Black"
                android:textSize="16dp"
                android:textStyle="bold" />

            <androidx.appcompat.widget.AppCompatEditText
                android:id="@+id/etDescSupportId"
                android:layout_width="match_parent"
                android:layout_height="match_parent"
                android:layout_marginTop="@dimen/_5sdp"
                android:layout_marginEnd="15sp"
                android:layout_marginStart="15sp"
                android:layout_below="@+id/tvdescriptionidd"
                android:background="@android:color/transparent"
                android:textColor="@color/lessBlack"
                android:textSize="14sp"
                android:textStyle="normal" />

        </RelativeLayout>


        <RelativeLayout
            android:id="@+id/rlsubmitbtnid"
            android:layout_width="@dimen/_130sdp"
            android:layout_height="@dimen/_45sdp"
            android:layout_below="@+id/rldescriptionid"
            android:layout_centerHorizontal="true"
            android:layout_centerVertical="true"
            android:layout_marginTop="15sp"
            android:background="@drawable/blankbtn">

            <androidx.appcompat.widget.AppCompatTextView
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="@dimen/_13sdp"
                android:textStyle="normal"
                android:textColor="@color/white"
                android:text="@string/send"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_9sdp"/>


        </RelativeLayout>


        <RelativeLayout
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_below="@+id/rlsubmitbtnid"
            android:layout_centerHorizontal="true"
            android:layout_margin="10sp">

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvheelpid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="@string/contact_the_heelp_team_by_e_mail"
                android:textColor="@color/Black"
                android:textSize="@dimen/_12sdp"
                android:textStyle="bold" />

            <androidx.appcompat.widget.AppCompatTextView
                android:id="@+id/tvmailidid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/tvheelpid"
                android:layout_centerHorizontal="true"
                android:text="@string/contact_heelp_com"
                android:textColor="@color/Black"
                android:textSize="@dimen/_11sdp"
                android:textStyle="bold" />


        </RelativeLayout>


    </RelativeLayout>

        </RelativeLayout>

    </ScrollView>
    <RelativeLayout
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_centerInParent="true"
        android:layout_centerHorizontal="true">

        <ProgressBar
            android:id="@+id/PbContactId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:visibility="gone" />

    </RelativeLayout>

</RelativeLayout>

