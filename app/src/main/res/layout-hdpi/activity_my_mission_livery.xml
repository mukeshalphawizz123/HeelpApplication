<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@color/white">




            <RelativeLayout
                android:id="@+id/rlHomeToolbarId"
                android:layout_width="match_parent"
                android:layout_height="160sp"
                android:background="@drawable/relativeredius">

                <androidx.appcompat.widget.AppCompatImageView
                    android:id="@+id/ivmissionliverydashboardbackId"
                    android:layout_width="35dp"
                    android:layout_height="35dp"
                    android:layout_marginStart="10dp"
                    android:layout_marginTop="25dp"
                    android:layout_marginEnd="10sp"
                    android:padding="6sp"
                    android:src="@drawable/back" />

                <RelativeLayout
                    android:layout_marginTop="@dimen/_10sdp"
                    android:id="@+id/rlnotificationHomeId"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content">


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ivnotificationId"
                        android:layout_width="@dimen/_30sdp"
                        android:layout_height="@dimen/_30sdp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:padding="@dimen/_3sdp"
                        android:src="@drawable/ring" />


                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvHomeNotificationCountId"
                        android:layout_width="@dimen/_20sdp"
                        android:layout_height="@dimen/_20sdp"
                        android:layout_alignParentEnd="true"
                        android:layout_marginTop="@dimen/_15sdp"
                        android:layout_marginEnd="@dimen/_10sdp"
                        android:background="@drawable/circle"
                        android:gravity="center"
                        android:visibility="gone"
                        android:textColor="@color/white"
                        android:textSize="@dimen/_10sdp"
                        android:textStyle="bold" />


                </RelativeLayout>

                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_below="@+id/rlnotificationHomeId"
                    android:layout_marginStart="60sp">


                    <androidx.appcompat.widget.AppCompatImageView
                        android:id="@+id/ivmissionid"
                        android:layout_width="70dp"
                        android:layout_height="70dp"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="10sp"
                        android:layout_marginEnd="15sp"
                        android:src="@drawable/linechartred" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvMyMissTitleId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerHorizontal="true"
                        android:layout_centerVertical="true"
                        android:layout_marginTop="15sp"
                        android:layout_toEndOf="@+id/ivmissionid"
                        android:fontFamily="@font/raleway_medium"
                        android:text="@string/mission_title"
                        android:textColor="@color/red"
                        android:textSize="@dimen/_16sdp"
                        android:textStyle="bold" />
                </RelativeLayout>

                <RelativeLayout
                    android:id="@+id/rlmissliveryviewdetailsid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_alignParentEnd="true"
                    android:layout_alignParentBottom="true"
                    android:layout_marginEnd="5sp"
                    android:layout_marginBottom="1dp">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvviewprofileid"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_centerVertical="true"
                        android:layout_marginEnd="@dimen/_2sdp"
                        android:text="@string/view_details"
                        android:textColor="@color/viewDetailColor"
                        android:textSize="14sp"
                        android:textStyle="bold" />

                    <androidx.appcompat.widget.AppCompatImageView
                        android:layout_width="13sp"
                        android:layout_height="11dp"
                        android:layout_centerVertical="true"
                        android:layout_toEndOf="@+id/tvviewprofileid"
                        android:src="@drawable/view_detail_arrow" />

                </RelativeLayout>

            </RelativeLayout>

    <ScrollView
        android:layout_width="match_parent"
        android:scrollbars="none"
        android:layout_below="@+id/rlHomeToolbarId"
        android:layout_height="match_parent">


        <RelativeLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:layout_marginBottom="40sp">


        <RelativeLayout
                android:id="@+id/rlcompletebtnid"
                android:layout_width="@dimen/_105sdp"
                android:layout_height="@dimen/_32sdp"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="5sp"
                android:background="@drawable/livree_box">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerInParent="true"
                    android:layout_marginTop="5sp"
                    android:paddingBottom="@dimen/_4sdp"
                    android:text="@string/LivrÃ©e"
                    android:textColor="@color/lightPink"
                    android:textSize="@dimen/_15sdp"
                    android:textStyle="bold" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlliverybackgroundid"
                android:layout_width="match_parent"
                android:layout_height="120dp"
                android:layout_below="@+id/rlcompletebtnid"
                android:layout_centerHorizontal="true"
                android:layout_margin="10sp"
                android:background="@drawable/completebackg">


                <RelativeLayout
                    android:id="@+id/rldummyimgid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="10sp">


                    <de.hdodenhof.circleimageview.CircleImageView
                        android:id="@+id/ivUserMyMissionDeliveryId"
                        android:layout_width="60sp"
                        android:layout_height="60sp" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvUserNameMyMissionDeliveryId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/ivUserMyMissionDeliveryId"
                        android:layout_centerHorizontal="true"
                        android:text="@string/zeeshan"
                        android:textColor="@color/bluee"
                        android:textSize="@dimen/_10sdp"
                        android:textStyle="bold" />

                </RelativeLayout>


                <RelativeLayout
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_centerHorizontal="true"
                    android:layout_centerVertical="true"
                    android:layout_marginStart="15sp"
                    android:layout_toEndOf="@+id/rldummyimgid">

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvtextidd"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:text="@string/my_comment"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/_11sdp"
                        android:textStyle="bold" />

                    <androidx.appcompat.widget.AppCompatTextView
                        android:id="@+id/tvCommentMyMissionDeliveryId"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_below="@+id/tvtextidd"
                        android:text="@string/i_finished_it"
                        android:textColor="@color/Black"
                        android:textSize="@dimen/_11sdp"
                        android:textStyle="normal" />


                </RelativeLayout>


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rlliveryid"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlliverybackgroundid">

                <androidx.recyclerview.widget.RecyclerView
                    android:id="@+id/rvLiveryfileuploadid"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginStart="@dimen/_10sdp"
                    android:layout_marginEnd="@dimen/_10sdp"
                    android:nestedScrollingEnabled="false" />

            </RelativeLayout>

            <RelativeLayout
                android:id="@+id/rlpaymentid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlliveryid"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="@dimen/_100sdp">

                <androidx.appcompat.widget.AppCompatTextView
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_bold"
                    android:text="@string/waiting_for_payment"
                    android:textColor="@color/purpleColor"
                    android:textSize="@dimen/_11sdp"
                    android:textStyle="bold" />


            </RelativeLayout>


            <RelativeLayout
                android:id="@+id/rlproblemid"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_below="@+id/rlpaymentid"
                android:layout_centerHorizontal="true"
                android:layout_marginTop="60sp"
                android:layout_marginBottom="@dimen/_100sdp">


                <androidx.appcompat.widget.AppCompatTextView
                    android:id="@+id/tvmymissionliverytextid"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:fontFamily="@font/raleway_medium"
                    android:text="@string/report_a_problem"
                    android:textColor="@color/lessBlack"
                    android:textSize="12sp"
                    android:textStyle="italic" />

            </RelativeLayout>


        </RelativeLayout>
    </ScrollView>
    <RelativeLayout
        android:layout_width="match_parent"
        android:layout_height="match_parent">

        <ProgressBar
            android:id="@+id/pbMymissionDeliveryId"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_centerInParent="true"
            android:visibility="invisible" />

    </RelativeLayout>

</RelativeLayout>




